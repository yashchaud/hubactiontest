# ============================================================================
# LIVEKIT CONFIGURATION
# ============================================================================
# Get these credentials from https://cloud.livekit.io/dashboard
# IMPORTANT: Use LIVEKIT_WS_URL (not LIVEKIT_URL) - must start with wss://
LIVEKIT_WS_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key_here
LIVEKIT_API_SECRET=your_api_secret_here

# ============================================================================
# SERVER CONFIGURATION
# ============================================================================
PORT=3001

# ============================================================================
# CONTENT CENSORSHIP CONFIGURATION
# ============================================================================
# Enable/disable AI-powered content censorship
ENABLE_CENSORSHIP=true

# RunPod GPU Service Configuration
# URL to your RunPod serverless endpoint for AI processing
# Example: https://xxxxx-8000.proxy.runpod.net
RUNPOD_SERVICE_URL=https://xxxxx.proxy.runpod.net

# ============================================================================
# CENSORSHIP AGENT CONFIGURATION
# ============================================================================
# Frame processing rate (5-10 FPS recommended to minimize latency)
# Lower FPS = lower latency but may miss rapid content changes
# Higher FPS = better detection but increased latency
AGENT_FRAME_RATE=10

# Censorship mode: blur, pixelate, or overlay
# - blur: Gaussian blur effect on detected regions (most natural)
# - pixelate: Mosaic/pixelation effect (retro style)
# - overlay: Solid black overlay (fastest, most aggressive)
AGENT_CENSORSHIP_MODE=blur

# Frame Processing Configuration
PROCESSING_FPS=10
MAX_RETRIES=3
RETRY_DELAY_MS=1000

# ============================================================================
# OPTIONAL: RECORDING CONFIGURATION
# ============================================================================
# Path for storing stream recordings (if recording is enabled)
RECORDING_STORAGE_PATH=./recordings

# ============================================================================
# OPTIONAL: WEBHOOK CONFIGURATION
# ============================================================================
# Enable webhook signature verification (recommended for production)
# Requires configuring webhook secret in LiveKit dashboard
ENABLE_WEBHOOK_VERIFICATION=false